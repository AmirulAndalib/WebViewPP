                    <com.google.android.material.textview.MaterialTextView android:id="@+id/app_resources_vconsole_text"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toTopOf="@id/app_resources_vconsole_version"

                        style="@style/ThemeComponentTextViewPrimary"
                        android:textAppearance="?textAppearanceBodyLarge"
                        android:text="@string/vconsole"
                        />
                    <androidx.appcompat.widget.AppCompatSpinner android:id="@+id/app_resources_vconsole_version"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/app_resources_vconsole_text"
                        app:layout_constraintBottom_toBottomOf="parent"
                        />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>
            <com.google.android.material.card.MaterialCardView android:id="@+id/app_resources_vconsole_plugin_sources_card"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                app:layout_columnSpan="1"
                app:layout_columnWeight="1"
                app:layout_gravity="fill"

                style="?attr/materialCardViewOutlinedStyle"
                android:clickable="true"
                >
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
