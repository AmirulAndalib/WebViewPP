            </com.google.android.material.card.MaterialCardView>
            <com.google.android.material.card.MaterialCardView android:id="@+id/app_resources_eruda_plugin_memory_card"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                app:layout_columnSpan="1"
                app:layout_columnWeight="1"
                app:layout_gravity="fill"

                style="?attr/materialCardViewOutlinedStyle"
                android:clickable="true"
                >
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    >
                    <com.google.android.material.textview.MaterialTextView android:id="@+id/app_resources_eruda_plugin_memory_text"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toTopOf="@id/app_resources_eruda_plugin_memory_version"

                        style="@style/ThemeComponentTextViewPrimary"
                        android:textAppearance="?textAppearanceBodyLarge"
