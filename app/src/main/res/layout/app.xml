                    <com.google.android.material.textview.MaterialTextView android:id="@+id/app_resources_vconsole_plugin_stats_text"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toTopOf="@id/app_resources_vconsole_plugin_stats_version"

                        style="@style/ThemeComponentTextViewPrimary"
                        android:textAppearance="?textAppearanceBodyLarge"
                        android:text="@string/vconsole_plugin_stats"
                        />
                    <androidx.appcompat.widget.AppCompatSpinner android:id="@+id/app_resources_vconsole_plugin_stats_version"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/app_resources_vconsole_plugin_stats_text"
                        app:layout_constraintBottom_toBottomOf="parent"
                        />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>
            <com.google.android.material.card.MaterialCardView android:id="@+id/app_resources_vconsole_plugin_vue_devtools_card"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                app:layout_columnSpan="1"
                app:layout_columnWeight="1"
                app:layout_gravity="fill"

                style="?attr/materialCardViewOutlinedStyle"
                android:clickable="true"
                >
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    >
                    <com.google.android.material.textview.MaterialTextView android:id="@+id/app_resources_vconsole_plugin_vue_devtools_text"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toTopOf="@id/app_resources_vconsole_plugin_vue_devtools_version"

                        style="@style/ThemeComponentTextViewPrimary"
                        android:textAppearance="?textAppearanceBodyLarge"
                        android:text="@string/vconsole_plugin_vue_devtools"
                        />
                    <androidx.appcompat.widget.AppCompatSpinner android:id="@+id/app_resources_vconsole_plugin_vue_devtools_version"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/app_resources_vconsole_plugin_vue_devtools_text"
                        app:layout_constraintBottom_toBottomOf="parent"
                        />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>
            <com.google.android.material.card.MaterialCardView android:id="@+id/app_resources_vconsole_plugin_outputlog_card"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                app:layout_columnSpan="1"
                app:layout_columnWeight="1"
                app:layout_gravity="fill"

                style="?attr/materialCardViewOutlinedStyle"
                android:clickable="true"
                >
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    >
                    <com.google.android.material.textview.MaterialTextView android:id="@+id/app_resources_vconsole_plugin_outputlog_text"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
